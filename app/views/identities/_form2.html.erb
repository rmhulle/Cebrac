
<div class="col-lg-12">

  <div class="col-lg-6">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h5>Basic form <small>Simple login form example</small></h5>
        </div>
        <div class="ibox-content">
        <div id="webcam"></div>


        <input type="button" value="Take picture" onclick="webcam.snap();"/>
        </div>
      </div>    
  </div>

   <div class="col-lg-6">
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h5>Basic form <small>Simple login form example</small></h5>
        </div>
        <div class="ibox-content">
            <div class="row">
           
              <%= simple_form_for(@identity, :html => {:multipart => true}) do |f| %>
                  <%= f.error_notification %>

                  <div class="form-inputs">
                    <%= f.input :name %>
                    <%= f.input :enrollment_id %>
                    <%= f.input :lecture %>
                    <%= f.input :expiration %>
                    <%= f.input :campus %>
                    <%= f.input :user_name %>
                    <%= f.input :user_id %>
                    <%= f.input :last_printed %>
                    <%= f.cropbox :avatar, :width => 500 %>
                    <%= f.crop_preview :avatar, :width => 150 %>
                    </div>

                  <div class="form-actions">
                    <%= f.button :submit %>
                  </div>
                <% end %>

            </div>
            </div>
        </div>
    </div>
</div>
