<div class="wrapper wrapper-content animated fadeInRight">

    <div class="col-lg-10">
        <h2>Cadatro de Alunos</h2>
        
</div>
<div class="col-lg-2">

</div>
        </div>
<div class="wrapper wrapper-content animated fadeInRight">
<div class="row">
<div class="col-lg-12">
<div class="ibox float-e-margins">
<div class="ibox-title">
    <h5>Encontre Alunos</h5>
    
</div>
<div class="ibox-content">

<table class="table table-striped table-bordered table-hover dataTables-example">
<thead>
<tr>
    <th>Name</th>
    <th>Enrollment</th>
    <th>Lecture</th>
    <th>Expiration</th>
    <th>Campus</th>
    <th>User name</th>
    <th>User</th>
    <th>Last printed</th>
    <th>Mostrar</th>
    <th>Editar</th>
    <th>Deletar</th>
</tr>
</thead>
<tbody>
    <% @identities.each do |identity| %>
      <tr>
        <td><%= identity.name %></td>
        <td><%= identity.enrollment_id %></td>
        <td><%= identity.lecture %></td>
        <td><%= identity.expiration %></td>
        <td><%= identity.campus %></td>
        <td><%= identity.user_name %></td>
        <td><%= identity.user_id %></td>
        <td><%= identity.last_printed %></td>
        <td><%= link_to 'Show', identity %></td>
        <td><%= link_to 'Edit', edit_identity_path(identity) %></td>
        <td><%= link_to 'Destroy', identity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
</tbody>
<tfoot>
<tr>
    <th>Name</th>
    <th>Enrollment</th>
    <th>Lecture</th>
    <th>Expiration</th>
      <th>Campus</th>
      <th>User name</th>
      <th>User</th>
      <th>Last printed</th>
      <th>Mostrar</th>
      <th>Editar</th>
      <th>Deletar</th>
</tr>
</tfoot>
</table>

</div>
</div>
</div>
</div>
</div>


